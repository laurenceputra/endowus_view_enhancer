{
  "name": "goal-portfolio-viewer",
  "version": "2.6.9",
  "description": "View and organize your investment portfolio by buckets with a modern interface",
  "main": "tampermonkey/goal_portfolio_viewer.user.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laurenceputra/goal-portfolio-viewer.git"
  },
  "keywords": [
    "goal-portfolio-viewer",
    "portfolio",
    "investment",
    "tampermonkey",
    "userscript"
  ],
  "author": "laurenceputra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laurenceputra/goal-portfolio-viewer/issues"
  },
  "homepage": "https://github.com/laurenceputra/goal-portfolio-viewer#readme",
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "coverageReporters": ["json-summary", "text"],
    "collectCoverageFrom": [
      "tampermonkey/**/*.user.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  }
}
